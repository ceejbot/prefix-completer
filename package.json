{
  "name": "prefix-completer",
  "description": "Simple redis-backed library for managing and querying an autocomplete dictionary.",
  "version": "0.2.4",
  "author": "C J Silverio <ceejceej@gmail.com>",
  "bugs": "http://github.com/ceejbot/prefix-completer/issues",
  "config": {
    "blanket": {
      "pattern": "completer.js",
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "output-reporter": "spec"
    },
    "travis-cov": {
      "threshold": 90
    }
  },
  "dependencies": {
    "async": "~1.5.0",
    "redis": "~2.4.2"
  },
  "optionalDependencies": {
    "hiredis": "~0.4.1"
  },
  "devDependencies": {
    "eslint": "~1.10.3",
    "mocha": "~2.3.4",
    "must": "~0.13.1",
    "nyc": "~4.0.1"
  },
  "keywords": [
    "redis",
    "autocomplete",
    "completer"
  ],
  "main": "completer",
  "repository": {
    "type": "git",
    "url": "git://github.com/ceejbot/prefix-completer.git"
  },
  "scripts": {
    "lint": "eslint *.js test/",
    "test": "nyc mocha -R spec test/test*.js",
    "travis": "npm run lint && npm test"
  }
}
